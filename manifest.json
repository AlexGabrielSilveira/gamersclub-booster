{
    "manifest_version": 2,
    "name": "GamersClub Booster",
    "description": "Extens√£o para o Google Chrome que adiciona algumas melhorias na plataforma da GamersClub.",
    "version": "1.0.1",
    "icons": {
      "16": "icons/icon_16.png",
      "48": "icons/icon_48.png",
      "128": "icons/icon_128.png"
    },
    "permissions": [
        "https://gamersclub.com.br/",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": ["*://*.gamersclub.com.br/jogador/*"],
            "js": ["lib/jquery.min.js", "profile.js"],
            "css" : ["profile.css"]
        },
		{
            "matches": ["*://*.gamersclub.com.br/minhas-partidas*"],
            "js": ["lib/jquery.min.js", "minhas-partidas.js"]
        },
		{
            "matches": ["*://*.gamersclub.com.br/team/matches/*"],
            "js": ["lib/jquery.min.js", "time-partidas.js"]
        },
		{
            "matches": ["*://*.gamersclub.com.br/lobby*"],
            "js": ["lib/jquery.min.js", "lobby.js"]
        }
    ],
    "options_page": "options.html"
}